pipeline {
    agent any

    stages {
        stage('validate') {
            steps {
                echo 'validating'
            }
        }
stage('compile') {
            steps {
                echo 'compiling'
}
}
stage('install') {
            steps {
                echo 'installing'
}
}
stage('deploy') {
            steps {
                echo 'deploying'
}
}
    }
}
